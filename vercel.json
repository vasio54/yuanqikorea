{
    "rewrites": [
      {
        "source": "/socket.io/(.*)",
        "destination": "/api/index"
      },
      {
        "source": "/(.*)",
        "destination": "/public/$1"
      },
      {
        "source": "/",
        "destination": "/public/index.html"
      }
    ],
    "functions": {
      "api/index.js": {
        "memory": 1024,
        "maxDuration": 30
      }
    }
  }